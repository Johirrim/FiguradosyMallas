      <h1>Mensajes</h1>
        <div class="row">
                <div class="col-lg-12">
                    <div class="table-responsive">        
                        <table id="example" class="table table-striped table-bordered" style="width:100%">
                        <thead>
                            <tr>
                                <th>Email</th>
                                <th>Nombre</th>
                                <th>Apellido</th>
                                <th>Mensaje</th>
                            </tr>
                        </thead>
                        <tbody>
                            {{#each Data}}
                            <tr>                              
                                <td>{{email}}</td>
                                <td>{{firstname}}</td>
                                <td>{{last}}</td>
                                <td>{{message}}</td>                              
                            </tr>
                            {{/each}}              
                        </tbody>        
                       </table>                  
                    </div>
                </div>
        </div>  

